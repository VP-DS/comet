{
    "name": "@comet/admin-icons",
    "version": "2.0.0",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/vivid-planet/comet-admin",
        "directory": "packages/admin/admin-icons"
    },
    "scripts": {
        "clean": "rimraf lib && rimraf src/generated",
        "generate-icons": "ts-node generate-icons.ts",
        "build": "yarn clean && yarn generate-icons && tsc",
        "start": "yarn clean && yarn generate-icons && tsc --project ./tsconfig.json --watch --preserveWatchOutput",
        "lint": "yarn lint:eslint && yarn lint:tsc",
        "lint:eslint": "eslint src/",
        "lint:tsc": "tsc --noEmit"
    },
    "peerDependencies": {
        "@mui/material": "^5.4.1",
        "react": "^17.0",
        "react-dom": "^17.0"
    },
    "devDependencies": {
        "@types/cli-progress": "^3.8.0",
        "cli-progress": "^3.8.2",
        "fast-xml-parser": "^3.18.0",
        "pascal-case": "^3.1.2",
        "rimraf": "^3.0.2",
        "ts-node": "^8.8.1",
        "typescript": "^4.2.2"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org",
        "access": "public"
    }
}
